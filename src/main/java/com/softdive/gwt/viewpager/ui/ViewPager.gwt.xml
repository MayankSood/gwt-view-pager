<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='ViewPager'>

	<inherits name='com.google.gwt.user.User' />

	<inherits name="com.googlecode.mgwt.MGWT" />
	<inherits name="com.googlecode.mgwt.linker.Linker" />
	<set-property name="mgwt.mainresource.inject" value="no" />

	<set-property name="user.agent" value="safari" />

	<inherits name="com.softdive.gwt.viewpager.ui.client.widget.carousel.Carousel" />
	
	<entry-point class='com.softdive.gwt.viewpager.ui.client.MadpiggyEntryPoint' />

	<inherits name="com.google.gwt.logging.Logging" />
	<set-property name="gwt.logging.enabled" value="TRUE" />
	<set-property name="gwt.logging.logLevel" value="ALL" />

	<source path='client' />
</module>

